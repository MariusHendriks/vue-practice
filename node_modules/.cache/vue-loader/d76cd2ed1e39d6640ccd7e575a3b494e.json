for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"formData.age\"\n          required\n          placeholder=\"Enter age\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\n\nexport default {\n  name: \"home\",\n  data() {\n    return {\n      showPetForm: false,\n      formData: {\n        name: \"\",\n        age: 0,\n        species: null,\n      },\n    };\n  },\n  computed: {\n    ...mapGetters([\"animalsCount\"]),\n  },\n  methods: {\n    ...mapActions([\"addPet\"]),\n    togglePetForm() {\n      this.showPetForm = !this.showPetForm;\n    },\n    handleSubmit() {\n      const { species, age, name } = this.formData;\n      const payload = {\n        species,\n        pet: {\n          name,\n          age,\n        },\n      };\n      this.addPet(payload);\n      this.formData = { name: \"\", age: 0, species: null };\n    },\n  },\n};\n</script>\n"]}]}